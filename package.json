{
  "name": "mat-markdown-editor",
  "description": "Material design markdown editor",
  "version": "0.0.8",
  "homepage": "https://github.com/michaeldoye/mat-markdown-editor",
  "author": {
    "name": "Michael Doye",
    "url": "https://github.com/michaeldoye"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/michaeldoye/mat-markdown-editor.git"
  },
  "license": "MIT",
  "keywords": [
    "markdown",
    "text editor",
    "angular",
    "material design"
  ],
  "bugs": {
    "url": "https://github.com/michaeldoye/mat-markdown-editor/issues"
  },
  "private": true,
  "scripts": {
    "start": "gulp build",
    "test": " gulp test",
    "demo": " gulp build:demo",
    "check-travis": " travis-status --repo=michaeldoye/mat-markdown-editor --branch=master --fail-pending || echo 'Not yet passing'"
  },
  "dependencies": {
    "@angular/cdk": "^7.3.3",
    "@angular/common": "7.2.8",
    "@angular/core": "7.2.8",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "^7.2.8",
    "@angular/material": "^7.3.3",
    "gh-got": "^8.1.0",
    "jest-cli": "^24.3.1",
    "tsickle": "^0.34.3"
  },
  "devDependencies": {
    "@angular/animations": "^7.2.8",
    "@angular/compiler": "7.2.8",
    "@angular/compiler-cli": "7.2.8",
    "@angular/platform-browser": "7.2.8",
    "@angular/platform-browser-dynamic": "7.2.8",
    "@angular/platform-server": "7.2.8",
    "@compodoc/gulp-compodoc": "0.0.10",
    "@types/jest": "25.2.0",
    "@types/lodash": "4.14.123",
    "@types/node": "11.11.0",
    "angular-cli-ghpages": "0.5.3",
    "angular2-template-loader": "0.6.2",
    "ansi-colors": "3.2.4",
    "autoprefixer": "9.5.0",
    "awesome-typescript-loader": "5.2.1",
    "codelyzer": "4.5.0",
    "commitplease": "3.2.0",
    "conventional-github-releaser": "^3.1.2",
    "core-js": "2.6.5",
    "css-loader": "2.1.1",
    "cssnano": "4.1.10",
    "del": "4.0.0",
    "fancy-log": "1.3.3",
    "gulp": "^3.9.1",
    "gulp-bump": "3.1.3",
    "gulp-conventional-changelog": "2.0.10",
    "gulp-coveralls": "0.1.4",
    "gulp-file": "0.4.0",
    "gulp-git": "2.9.0",
    "gulp-hub": "4.2.0",
    "gulp-inline-ng2-template": "5.0.1",
    "gulp-tslint": "8.1.4",
    "html-loader": "0.5.5",
    "jest": "24.3.1",
    "jest-preset-angular": "7.0.1",
    "lodash": "4.17.11",
    "node-sass": "4.11.0",
    "postcss": "7.0.14",
    "postcss-strip-inline-comments": "0.1.5",
    "prettier": "1.16.4",
    "pump": "3.0.0",
    "raw-loader": "1.0.0",
    "rollup": "0.58.2",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-uglify": "3.0.0",
    "run-sequence": "2.2.1",
    "rxjs": "6.4.0",
    "sass-loader": "7.1.0",
    "source-map-loader": "0.2.4",
    "to-string-loader": "1.1.5",
    "travis-status": "3.0.1",
    "tslint": "5.13.1",
    "typescript": "3.1.1",
    "yargs": "13.2.2",
    "zone.js": "0.8.29"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "roots": [
      "<rootDir>/src/"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/config",
      "/dist/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/setupJest.ts"
  },
  "greenkeeper": {
    "ignore": [
      "@angular/core",
      "@angular/common",
      "@angular/compiler",
      "@angular/platform-server",
      "@angular/platform-browser",
      "@angular/platform-browser-dynamic",
      "@angular/compiler-cli",
      "zone.js",
      "rxjs",
      "tslint",
      "gulp-tslint",
      "typescript",
      "awesome-typescript-loader",
      "codelyzer",
      "@angular/animations",
      "@types/jasmine",
      "gulp",
      "rollup",
      "rollup-plugin-commonjs",
      "rollup-plugin-node-resolve",
      "rollup-plugin-uglify"
    ]
  },
  "commitplease": {
    "style": "angular",
    "types": [
      "feat",
      "fix",
      "docs",
      "style",
      "refactor",
      "perf",
      "test",
      "chore",
      "revert",
      "demo"
    ],
    "scope": "\\S+.*"
  },
  "prettier": {
    "bracketSpacing": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "htmlWhitespaceSensitivity": "css"
  }
}
